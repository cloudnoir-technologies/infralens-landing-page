{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".", // Base directory for resolving non-relative module names
        "paths": {
          "@components/*": ["src/components/*"],
          "@assets/*": ["src/assets/*"]
    }
  }
}